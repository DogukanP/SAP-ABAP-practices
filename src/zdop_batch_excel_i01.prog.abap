MODULE user_command_0100 INPUT.
  CLEAR ok_code.
  ok_code = sy-ucomm.
  CASE ok_code.
    WHEN 'BACK' OR 'CANCEL'.
      LEAVE TO SCREEN 0.
    WHEN 'EXIT' .
      LEAVE PROGRAM.
  ENDCASE.
ENDMODULE.
